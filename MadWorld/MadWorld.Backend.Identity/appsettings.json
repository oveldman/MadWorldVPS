{
  "DbContext": {
    "IdentityConnectionString": "Host=postgres-loadbalancer;Port=5432;Database=MadWorld_Identity;Username=admin;Password={password};",
    "Password": "Empty"
  },
  "Jwt": {
    "Key": "Empty",
    "Issuer": "https://identity.mad-world.nl/",
    "Audience": "https://api.mad-world.nl/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Exceptions", "Serilog.Enrichers.ClientInfo" ],
    "Enrich": [ "WithExceptionDetails", "WithClientIp" ],
    "WriteTo": [
      { 
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq-loadbalancer:5341"
        }
      }
    ],
    "Properties": {
      "Application": "MadWorld.Backend.Identity"
    }
  },
  "AllowedHosts": "*"
}
